{
  "name": "prosektorweb_dashboard",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "pnpm --filter web dev",
    "dev:web": "pnpm --filter web dev",
    "validate:agents-team": "node scripts/validate-agents-team.mjs",
    "validate:agents-team:codebase-control": "node scripts/validate-codebase-control-team.mjs",
    "inventory:backend": "node scripts/generate-backend-hardening-inventory.mjs",
    "inventory:backend:check": "node scripts/generate-backend-hardening-inventory.mjs --check",
    "db:migrations:sync-report": "node scripts/check-migration-sync.mjs",
    "db:migrations:sync-check": "node scripts/check-migration-sync.mjs --strict",
    "build": "pnpm --filter web build",
    "lint": "pnpm --filter web lint",
    "test:contracts": "pnpm --filter @prosektor/contracts test",
    "test:db": "pnpm --filter @prosektorweb/testing test:db",
    "test:web": "pnpm --filter web test",
    "test:e2e": "pnpm --filter web test:e2e",
    "security:audit": "pnpm audit --audit-level=moderate",
    "security:all": "pnpm security:audit",
    "hygiene:repo": "pnpm lint && pnpm build",
    "test:strict": "pnpm run test:web",
    "hygiene:git": "pnpm run hygiene:repo && pnpm run test:strict"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.27.3"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-slider": "^1.3.6",
    "zustand": "^5.0.11"
  }
}